@charset "UTF-8";

#bodies {
	display: block;
	position: relative;
}

#conts {
	margin: 0 auto;
    width: 1008px;
    min-height: 800px;
    padding: 10px 0 0 0;
    margin-top: 20px;
}

#conts > ul {
	list-style: none;
}

#conts > ul li {
	display: inline-block;
	vertical-align: middle;
}

.new_album {
	width: 477px;
	overflow: hidden;
	border: 0;
}

.new_album .album_list {
	height: 355px;
	border: 1px solid #ddd;
	position: relative;
}

.mySlides_div {
	width: 10000px;
	height: 322px;
}

.new_album .album_list ul {
	float: left;
	width: 477px;
	height: 317px;
	list-style: none;
	position: relative;
}

.new_album .album_list ul li {
	display: inline-block;
	width: 156px;
	height: 156px;
	position: relative;
}

.mySlides li:nth-child(4), .mySlides li:nth-child(5), .mySlides li:nth-child(6) {
	margin-top: 4px;
}

.album_li .poster {
	width: 156px;
	height: 156px;
}

.new_album .album_list ul li > span {
	position: absolute;
	bottom: 0px;
	display: block;
    width: 136px;
	height: 22px;
    padding: 8px 10px 0 10px;
    background: url(/Melon/img/main/album/bg_frame.png) no-repeat 0 -682px;
    text-align: center;
    color: #fff;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    word-wrap: normal;
    cursor: pointer;
}

.new_album .album_list ul li > span a {
	color: #fff;
	text-decoration: none;
}

.new_album .album_list ul li > span:hover {
	text-decoration: underline;
}

.new_album .album_list h2 {
	display: inline-block;
	position: absolute;
	height: 24px;
    color: #1a1a1a;
    font-size: 15px;
    line-height: 15px;
    letter-spacing: -1px;
    font-weight: bold;
    font-family: "맑은 고딕","Malgun Gothic","Apple Gothic",sans-serif;
    margin-left: 6px;
    margin-top: 3px;
}

.new_album .album_list span.pageBtn {
	display: inline-block;
	position: absolute;
	right: 0;
	height: 24px;
	line-height: 24px;
	text-align: right;
}

.new_album .album_list h2 a {
	text-decoration: none;
    overflow: hidden;
    width: 70px;
    height: 15px;
    display: block;
    background: url(/Melon/img/main/album/bg_main_title_140716.png) no-repeat;
    background-position: 0 -15px;
    text-indent: -9999px;
}

.event {
	width: 236px;
	height: 355px;
	border: 1px solid #ddd;
}

.event img {
	width: 236px;
	height: 315px;
}

.login_wrap_mt {
	display: inline-block;
	height: 355px;
    width: 280px;
    border: none;
    margin: 0;
    padding: 0;
}

.main_banner {
    margin-top: 30px;
}

.login_button {
    height: 54px;
    display: inline-block;
    margin: 24px 0 0 0;
    padding: 0;
    background: 0;
    border: 0;
    text-decoration: none !important;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    cursor: pointer;
}

.login_button span {
	display: block;
    position: relative;
	width: 238px;
    height: 54px;
    background: url(/Melon/img/main/login/btn_login_main.png) no-repeat;
    text-indent: -9999px;
}

.login_mt {
    width: 282px;
    height: 135px;
    border: 1px solid #ddd;
    background: #f8f8f8;
    text-align: center;
}

.login_area {
	margin-top: 13px;
}

.login_area a {
	padding: 0 10px;
	text-decoration: none;
    font-size: 12px;
    line-height: 14px;
    font-family: "돋움",Dotum,"Apple Gothic",sans-serif;
    color: #969696;
    letter-spacing: -1px;
}

.login_area a:hover {
	text-decoration: underline;
}

.login_area a:last-child {
	margin-left: -2px;
    background: url(/Melon/img/main/login/bg_gray_line.png) no-repeat left 2px;
}

.promotion_mt {
	width: 282px;
	margin-top: 24px;
	border: 1px solid #ddd;
}

.prev span {
	display: inline-block;
	width: 20px;
    height: 17px;
    background: url(/Melon/img/main/album/bg_btn_page02_140716.png) no-repeat;
    background-position: left -238px;
    font-size: 0;
    line-height: 0;
}

.next span {
	display: inline-block;
	position: relative;
	left: -6px;
	width: 20px;
    height: 17px;
    background: url(/Melon/img/main/album/bg_btn_page02_140716.png) no-repeat;
    background-position: right -238px;
    font-size: 0;
    line-height: 0;
}



/* 로그아웃 */
.logout_mt {
    width: 282px;
    height: 135px;
    border: 1px solid #ddd;
    background: #f8f8f8;
    text-align: center;
    position: relative;
}

.mem_info {
    height: 30px;
    padding: 10px 6px 0 6px;
    background: #f3f3f3;
    position: relative;
}

.mem_info strong {
	float: left;
    display: inline-block;
    max-width: 108px;
    padding: 2px 5px 0 0;
    color: #1a1a1a;
    font-size: 12px;
}

.mem_info strong a {
    display: inline-block;
    width: auto;
    max-width: 92px;
    margin: -2px 0 0 0;
    padding: 0;
    padding-top: 2px;
    padding-bottom: 1px;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-wrap: normal;
    overflow: hidden;
    vertical-align: top;
    text-decoration: none;
}

.mem_info > a {
	display: inline-block;
    float: left;
    margin: 0 0 0 5px;
    padding: 2px 0 0;
    font-size: 11px;
    font-family: "Dotum","Apple Gothic",sans-serif;
    color: #7d7d7d;
    text-decoration: underline;
}

.mem_info .btn_logout {
    float: right;
    width: 41px;
    margin: 0;
    padding: 1px 5px 0 5px;
    border: 1px solid #d9d9d9;
    line-height: 17px;
    background-color: #fff;
    letter-spacing: -1px;
    text-decoration: none;
}

.mem_used {
	border-top: 1px solid #d8dadb;
	padding-top: 10px;
}

.btn_buy strong {
	display: inline-block;
    margin: 0 10px;
    padding: 14px 0 7px 7px;
    background: url(/Melon/img/main/login/bg_dot_01.png) no-repeat left 21px;
    font-weight: normal;
    vertical-align: top;
}

.btn_buy > span {
	display: inline-block;
    padding: 14px 0 7px 0px;
    font-weight: normal;
    vertical-align: top;
}

.btn_buy > span a {
	color: #000;
}

.mem_used > ul {
	overflow: hidden;
    margin-left: 25px;
    padding-top: 7px;
}

.mem_used > ul li {
	float: left;
    padding: 0 10px;
    background: url(/Melon/img/main/login/bg_dot_01.png) no-repeat left 5px;
    line-height: 13px;
    margin-right: 30px;
}

.mem_used  > ul li > a {
    text-decoration: none;
    font-size: 11px;
    line-height: 13px;
    font-family: "Dotum","Apple Gothic",sans-serif;
    vertical-align: top;
}

.mem_used .label {
	width: 20px;
    height: 10px;
    color: #777;
}

.mem_used .nm {
	color: #333;
	margin-left: 5px;
}


/* 이슈 */
.issue {
	margin-top: 20px;
	list-style: none;
	width: 100%;
}

.issue > li {
	display: inline-block;
	vertical-align: middle;
}

.hot_issue {
	position: relative;
}

.hot_issue h2 {
	border-bottom: 1px solid #dfe6e8;
	height: 24px;
    color: #1a1a1a;
    font-size: 15px;
    line-height: 15px;
    letter-spacing: -1px;
    font-weight: bold;
    font-family: "맑은 고딕","Malgun Gothic","Apple Gothic",sans-serif;
}

.hot_issue h2 span.pageNumber {
	font-size: 12px;
	font-weight: 400;
	color: #666;
	margin-right: 3px;
}

.hot_issue h2 span.title_link {
	width: 70px;
    height: 15px;
    display: inline-block;
    background: url(/Melon/img/main/album/bg_main_title_140716.png) no-repeat;
    background-position: 0 -194px;
    text-indent: -9999px;
}

.hot_issue h2 span.txt_intro {
	display: inline-block;
    width: 300px;
    margin-top: 1px;
    padding-left: 12px;
    letter-spacing: 0;
    font-weight: normal;
    color: #888;
    font-size: 12px;
    font-family: "Dotum","Apple Gothic",sans-serif;
    background: 0;
    vertical-align: top;
}

.btn_issue {
	position: absolute;
	right: 5px;
}

.hot_issue > ul {
 	width: 701px;
 	overflow: hidden;
}

.issue_list {
	width: 10000px;
	float: left;
}

.issue_div {
	float: left;
	width: 701px;
	position: relative;
}

.issue_list dl {
	position: relative;
    margin: 16px 0 2px 16px;
    float: left;
}

.issue_list dl:nth-child(3n+1) {
	margin-left: 0px;
}

.issue_list dl dd {
	width: 223px;
    height: 148px;
}

.issue_list dl dt {
    width: 223px;
    line-height: 16px;
}

.issue_list dl dt a {
	text-decoration: none;
    color: #888;
    cursor: pointer;
}

.issue_list dl dt a:hover {
	text-decoration: underline;
}

.issue_list .title {
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    height: 16px;
    font-weight: bold;
    color: #333;
    margin-top: 6px;
    margin-bottom: 2px;
    word-break: break-all;
}

.issue_list .ellipsis {
    display: block;
    width: 223px;
    height: 16px;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
}

.issue_list .view_no {
    margin-top: 2px;
    font-size: 11px;
    font-family: "Dotum","Apple Gothic",sans-serif;
    color: #888;
    height: 14px;
}

.issue_list .view_no a {
    color: #7cb710;
    text-decoration: none;
}

.issue_list .view_no a:hover {
	text-decoration: underline;
}

.issue_list .view_no .bar {
    padding-right: 6px;
    margin-right: 5px;
    background: url(/Melon/img/main/issue/bg_gray_line.png) no-repeat right 1px;
    color: #7cb710;
}


