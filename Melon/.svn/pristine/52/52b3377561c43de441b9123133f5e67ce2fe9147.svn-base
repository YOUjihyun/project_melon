.heart {
	background: url(/Melon/img/magazine/btn_colk1_sprite.png) no-repeat 0 0;
    width: 11px;
    height: 11px;
    display: inline-block;
    vertical-align: middle;
}

.seperator {
	margin: 0px 0px 0px 3px;
	padding: 0px 0px 0px 8px;
	background: url('http://cdnimg.melon.co.kr/resource/image/web/musicstory/bl_musictoday.jpg') 0px 50% no-repeat;
}

.page{
	margin-top: 24px;
	text-align: center;
}

.page_btn_first {
	vertical-align: middle;
	color: #1a1a1a;
	display: inline-block;
	position: relative;
	width: 24px; 
	height: 24px;
	background: url('http://cdnimg.melon.co.kr/resource/image/web/common/btn_paginate.png') 0px -108px no-repeat;
}

.page_btn_pre {
	vertical-align: middle;
	color: #1a1a1a;
	display: inline-block;
	position: relative;
	width: 24px;
	height: 24px;
	background: url('http://cdnimg.melon.co.kr/resource/image/web/common/btn_paginate.png') -26px -108px no-repeat;
}

.page_num_wrap {
	margin: 0px 0px 0px 2px;
	line-height: 1.2em;
	font-family: 돋움, dotum, sans-serif, 'Apple Gothic', sans-serif;
	vertical-align: middle;
	display: inline-block;
}

.page_num {
	position: relative;
	display: inline-block;
	padding: 5px 6px 3px;
	border: 1px solid #80ba00;
	min-width: 10px;
	margin-left: -1px;
	color: #80ba00;
	vertical-align: middle;
	z-index: 2;
}

.page_btn_next {
	vertical-align: middle;
	color: #1a1a1a;
	display: inline-block;
	position: relative;
	width: 24px; 
	height: 24px;
	background: url('http://cdnimg.melon.co.kr/resource/image/web/common/btn_paginate.png') -84px -108px no-repeat;}

.page_btn_last {
	vertical-align: middle;
	color: #1a1a1a;
	display: inline-block;
	position: relative;
	width: 24px; 
	height: 24px;
	background: url('http://cdnimg.melon.co.kr/resource/image/web/common/btn_paginate.png') -110px -108px no-repeat;
}

.writeBtn {
	float: right;
	border: 1px solid black;
	padding: 0 3px 0 3px;
	text-decoration: none;
	margin-right: 10px;
}



