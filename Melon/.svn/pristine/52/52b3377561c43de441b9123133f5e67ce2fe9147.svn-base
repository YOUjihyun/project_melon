.wrap {
	padding: 0;
	margin:0;
}

.wrap2 {
	margin:0 auto;
	position: relative;
	display: block;
	width: 1008px;
	padding: 52px 0 88px;
}

.title_wrap2 {
	margin: 25px 0px 0px;
	padding: 0px 0px 6px;
	border-width: 0px 0px 1px;
	border-image: initial;
	font-variant-numeric: inherit;
	font-stretch: inherit;
	font-size: 12px;
	line-height: inherit;
	font-family: 돋움, Dotum, '맑은 고딕', 'Malgun Gothic', 'Apple Gothic', sans-serif;
	vertical-align: baseline;
	overflow: hidden;
	color: #666666;
	border-color: initial initial #848484 initial;
	border-style: initial initial solid initial;
}

.title2 {
	font-weight: bold;
	font-size: 18px;
	font-family: '맑은 고딕', 'Malgun Gothic', 돋움, Dotum, 'Apple Gothic', sans-serif;
	color: #1a1a1a;
	float: left;
}

